FROM            SIG_DETALLE_PEDIDOS INNER JOIN
                         CATALOGO_BIEN_SERV ON SIG_DETALLE_PEDIDOS.sec_ejec = CATALOGO_BIEN_SERV.SEC_EJEC AND 
                         SIG_DETALLE_PEDIDOS.TIPO_BIEN = CATALOGO_BIEN_SERV.TIPO_BIEN AND SIG_DETALLE_PEDIDOS.GRUPO_BIEN = CATALOGO_BIEN_SERV.GRUPO_BIEN AND
