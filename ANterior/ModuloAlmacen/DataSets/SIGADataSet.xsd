                    ISNULL( cast( SIG_PEDIDOS.MOTIVO_PEDIDO as Varchar(200)),'Ninguno') 'MOTIVO_PEDIDO'
FROM            SIG_DETALLE_PECOSA INNER JOIN
                      SIG_DETALLE_PEDIDOS ON SIG_DETALLE_PECOSA.ANO_EJE = SIG_DETALLE_PEDIDOS.ANO_EJE AND 
                         SIG_DETALLE_PECOSA.sec_ejec = SIG_DETALLE_PEDIDOS.sec_ejec AND SIG_DETALLE_PECOSA.TIPO_BIEN = SIG_DETALLE_PEDIDOS.TIPO_BIEN AND 
                         SIG_DETALLE_PECOSA.TIPO_PEDIDO = SIG_DETALLE_PEDIDOS.TIPO_PEDIDO AND 
                         SIG_DETALLE_PECOSA.NRO_PEDIDO = SIG_DETALLE_PEDIDOS.NRO_PEDIDO AND 
                         SIG_DETALLE_PECOSA.SECUENCIA = SIG_DETALLE_PEDIDOS.SECUENCIA INNER JOIN
                        SIG_PEDIDOS ON SIG_DETALLE_PEDIDOS.ANO_EJE = SIG_PEDIDOS.ANO_EJE AND SIG_DETALLE_PEDIDOS.sec_ejec = SIG_PEDIDOS.SEC_EJEC AND 
                         SIG_DETALLE_PEDIDOS.TIPO_BIEN = SIG_PEDIDOS.TIPO_BIEN AND SIG_DETALLE_PEDIDOS.TIPO_PEDIDO = SIG_PEDIDOS.TIPO_PEDIDO AND 
                         SIG_DETALLE_PEDIDOS.NRO_PEDIDO = SIG_PEDIDOS.NRO_PEDIDO INNER JOIN
                         [10.19.0.13].[SIGA].[dbo].SIG_PERSONAL ON SIG_PEDIDOS.SEC_EJEC = SIG_PERSONAL.sec_ejec AND SIG_PEDIDOS.EMPLEADO = SIG_PERSONAL.empleado
WHERE        (SIG_PEDIDOS.ACT_PROY = @ActProy) AND (SIG_PEDIDOS.ANO_EJE = @AnioEje) AND (SIG_PEDIDOS.TIPO_BIEN = 'B')
GROUP BY SIG_PEDIDOS.SEC_EJEC, SIG_PEDIDOS.ANO_EJE, SIG_PEDIDOS.TIPO_PEDIDO, SIG_PEDIDOS.TIPO_BIEN, SIG_PEDIDOS.NRO_PEDIDO, 
                         SIG_DETALLE_PECOSA.NRO_PECOSA, SIG_PERSONAL.nombre_completo, SIG_PEDIDOS.FECHA_REG, ISNULL( cast( SIG_PEDIDOS.MOTIVO_PEDIDO as Varchar(200)),'Ninguno')</CommandText>
