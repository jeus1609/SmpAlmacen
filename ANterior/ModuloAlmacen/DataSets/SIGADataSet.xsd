                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CATALOGO_BIEN_SERV.CODIGO_ITEM, CATALOGO_BIEN_SERV.NOMBRE_ITEM, UNIDAD_MEDIDA.NOMBRE AS 'UNIDAD', SIG_DETALLE_PEDIDOS.PRECIO_UNIT, 
