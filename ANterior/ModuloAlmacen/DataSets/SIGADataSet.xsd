                         SIG_DETALLE_PEDIDOS.ITEM_BIEN = CATALOGO_BIEN_SERV.ITEM_BIEN INNER JOIN
                         UNIDAD_MEDIDA ON SIG_DETALLE_PEDIDOS.UNIDAD_MEDIDA = UNIDAD_MEDIDA.UNIDAD_MEDIDA
where SIG_DETALLE_PEDIDOS.ano_eje = @ano_eje  and SIG_DETALLE_PEDIDOS.sec_ejec = @sec_ejec  and SIG_DETALLE_PEDIDOS.tipo_bien = @tipo_bien  and SIG_DETALLE_PEDIDOS.tipo_pedido = @tipo_pedido and SIG_DETALLE_PEDIDOS.Nro_Pedido = @nro_pedido</CommandText>
